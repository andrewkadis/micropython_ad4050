    "version": "0.2.0",
        "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "jlink",
            "cwd": "${workspaceRoot}",
            "executable": "./bin/micropython.elf",
            "name": "AD4050LZ Debug",
            "device": "ADUCM4050",
            "interface": "swd",
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args" : ["", ""]
        },
    ]
}